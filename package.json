{
  "name": "next-boilerplate",
  "version": "0.0.2",
  "description": "Next.js boilerplate",
  "main": "index.js",
  "repository": "git@github.com:GDR/next-boilerplate.git",
  "author": "Damir Garifullin gosugdr@gmail.com",
  "license": "MIT",
  "scripts": {
    "open": "next src",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "lint": "esw src",
    "lint:watch": "yarn lint --watch",
    "start": "concurrently -k -r -s first --names \"\" \"yarn open\" \"yarn test:watch\" \"yarn run lint:watch\"",
    "prod": "yarn prod:build && yarn prod:start",
    "prod:build": "cross-env NODE_ENV=production next build src",
    "prod:start": "cross-env NODE_ENV=production next start"
  },
  "dependencies": {
    "next": "^5.1.0",
    "next-redux-wrapper": "^1.3.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-jss": "^8.3.5",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-plugin-inline-react-svg": "^0.5.2",
    "babel-plugin-transform-decorators": "^6.24.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-watch": "^3.1.4",
    "jest": "^22.4.3",
    "prop-types": "^15.6.1",
    "react-test-renderer": "^16.3.0",
    "redux-devtools": "^3.4.1",
    "redux-devtools-extension": "^2.13.2",
    "webpack": "^3.0.0"
  }
}
